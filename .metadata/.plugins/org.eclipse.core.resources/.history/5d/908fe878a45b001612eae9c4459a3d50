package du.iit.dhaka;

import java.security.PublicKey;

public class Recarsion {

	public static int recursive(int number){
		
		
		if(number<=10){
			System.out.println(number);
		return recursive(number+1);
		}
		return 0;
	}
	public static void main(String[] args) {
		
		recursive(1);
			
			
		

	}

}
